{
  "Database": {
    "Provider": "Sqlite"
  },
  "Ui": {
    "Themes": {
      "Admin": { "NavbarClass": "navbar-dark bg-dark" },
      "Host": { "NavbarClass": "navbar-dark bg-primary" },
      "Participant": { "NavbarClass": "navbar-dark bg-success" }
    }
  },
  "ConnectionStrings": {
    "CohortWeb": "Data Source=cohort-web.db"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:5003",
      "https://localhost:4200",
      "https://localhost:5001"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
